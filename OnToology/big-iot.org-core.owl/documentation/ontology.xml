        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EndpointType</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/ExternalEndpoint -->


    <Class rdf:about="http://schema.big-iot.org/core/ExternalEndpoint">
