        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AccessInterfaceType</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/BasicData -->


    <Class rdf:about="http://schema.big-iot.org/core/BasicData">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gives a detail description of the transported payload data between consumer and provider based on this offering</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BasicData</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/Categorization -->


    <Class rdf:about="http://schema.big-iot.org/core/Categorization">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdf:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Hierarchy of offering categories</rdf:comment>
        <rdf:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Categorization</rdf:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/Consumer -->


    <Class rdf:about="http://schema.big-iot.org/core/Consumer">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consumers discover and subscribe to offerings of interest via a marketplace in order to access the resources. A consumer can be either an application or service instance that requires access to IoT resources in order to implement an intended service or function (e.g., a smart parking service provided by the city)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consumer</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/DataTypeAnnotation -->


    <Class rdf:about="http://schema.big-iot.org/core/DataTypeAnnotation">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Anything about which information is provided via an offering.</rdfs:comment>
    </Class>
    


    <!-- http://schema.big-iot.org/core/Endpoint -->


    <Class rdf:about="http://schema.big-iot.org/core/Endpoint">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The endpoint includes all relevant information which is required to setup right communication to the provider&apos;s offering.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Endpoint</rdfs:label>
    </Class>
    


