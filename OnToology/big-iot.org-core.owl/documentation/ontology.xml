    <!-- http://schema.big-iot.org/core/AccessInterfaceType -->


    <Class rdf:about="http://schema.big-iot.org/core/AccessInterfaceType">
        <rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
