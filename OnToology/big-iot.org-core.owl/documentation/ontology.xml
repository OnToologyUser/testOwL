    <!-- http://schema.big-iot.org/core/Offering -->


    <Class rdf:about="http://schema.big-iot.org/core/Offering">
        <rdfs:subClassOf rdf:resource="http://schema.org/Offer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An offering encompasses a set of IoT resources, typically a set of related information (e.g. low-level sensor data like temperature or aggregate information across a region) or functions (e.g. actuation tasks like open a gate or computational functions like computing route), that are offered on a marketplace.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Offering</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/OfferingCategory -->


    <Class rdf:about="http://schema.big-iot.org/core/OfferingCategory">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category of an offering.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OfferingCategory</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/OfferingQuery -->


    <Class rdf:about="http://schema.big-iot.org/core/OfferingQuery">
        <rdfs:subClassOf rdf:resource="http://schema.org/Demand"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consumers discover offerings they&apos;re interesting through the marketplace by providing an (offering) query. The query entails a specification of the type of offerings the consumer is interested in</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OfferingQuery</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/OfferingSubscription -->


    <Class rdf:about="http://schema.big-iot.org/core/OfferingSubscription">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subscription of a consumer to a specific offering registered by a provider.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OfferingSubscription</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/PricingModel -->


    <Class rdf:about="http://schema.big-iot.org/core/PricingModel">
        <rdfs:subClassOf rdf:resource="http://schema.org/Enumeration"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pricing models include per byte, per access or per month pricing. Offerings can also be free.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PricingModel</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/Provider -->


    <Class rdf:about="http://schema.big-iot.org/core/Provider">
        <rdfs:subClassOf rdf:resource="http://schema.org/Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Providers register their offerings on a marketplace and provides access to the offered resources via a common API. A provider can be either a platform or a service instance that offers available resources, i.e., some information or access to functions that it wants to share or trade on the marketplace (e.g. an IoT platform of a parking lot provider)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Provider</rdfs:label>
    </Class>
    


    <!-- http://schema.big-iot.org/core/ProxyEndpoint -->


