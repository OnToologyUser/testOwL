                                owl:onProperty :hasPrivacyPolicy ;
                                owl:allValuesFrom :PrivacyPolicy
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasRoleSet ;
                                owl:allValuesFrom :RoleSet
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasSecurityPolicy ;
                                owl:allValuesFrom :SecurityPolicy
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasUserAssignmentSet ;
                                owl:allValuesFrom :UserAssignmentSet
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasConstraintCatalog ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ConstraintCatalog
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasRoleSet ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :RoleSet
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasUserAssignmentSet ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :UserAssignmentSet
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDigitalSignature ;
                                owl:allValuesFrom xsd:base64Binary
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDomainName ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasIdentifier ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasName ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasName ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ;
              <http://purl.org/dc/elements/1.1/description> "An organization is an administrative and/or functional entity."^^xsd:string ;
              <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
              rdfs:comment "In the Composite Security and Privacy DAM, a generalization of JurisdictionalOrganization and ProviderOrganization is named 'Authority'.  However, ISO 22600-2 defines Authority as 'entity that is responsible for the issuance of certificates'."^^xsd:string ,
                           "It is expected that specific organizations will create and maintain their own sub-ontology which imports SecurityAndPrivacyOntology.owl along with some or all of the accompanying subontologies and adds suitable local instances of classes."^^xsd:string ,
                           "Organization has exactly one role set per conversation with M. Davis et al., 08/24/2010."^^xsd:string ,
                           "Textual description derived from Merriam Webster."^^xsd:string ;
              rdfs:label "Organization"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#PaymentSource
