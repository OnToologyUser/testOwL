                                                        owl:unionOf ( :InterorganizationalAgreement
                                                                      :Jurisdiction
                                                                      :Organization
                                                                      :Person
                                                                    )
                                                      ]
                                  ] ;
                  <http://purl.org/dc/elements/1.1/description> "The PrivacyOperation class categorizes operations from the perspective of their effect on the privacy of (health) information."^^xsd:string ;
