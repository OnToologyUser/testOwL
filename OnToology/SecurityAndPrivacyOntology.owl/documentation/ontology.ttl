                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:onDatatype xsd:integer ;
                                    owl:withRestrictions ( [ xsd:minInclusive 0
                                                           ]
                                                           [ xsd:maxInclusive 86400
                                                           ]
                                                         )
                                  ] .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#authenticationLevelOfAssurance
:authenticationLevelOfAssurance rdf:type owl:DatatypeProperty ;
                                rdfs:subPropertyOf :levelOfAssurance ;
                                rdf:type owl:FunctionalProperty .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#governsTimeOfDay
:governsTimeOfDay rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :BasicSecurityPolicy ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:integer ;
                               owl:withRestrictions ( [ xsd:minInclusive 0
                                                      ]
                                                      [ xsd:maxInclusive 86400
                                                      ]
                                                    )
                             ] .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasDigitalSignature
:hasDigitalSignature rdf:type owl:DatatypeProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :Organization
                                                 :Person
                                               )
                                 ] ;
                     rdfs:range xsd:base64Binary .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasDomainName
:hasDomainName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Organization ;
               rdfs:range xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasEffectiveTime
:hasEffectiveTime rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :Policy
                                              :PrivacyConsentDirective
                                              :PrivacyRule
                                            )
                              ] ;
                  rdfs:range xsd:dateTime .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasIdentifier
:hasIdentifier rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Object
                                           :Organization
                                           :Person
                                           :Policy
                                           :PrivacyConsentDirective
                                           :UserIdentity
                                         )
                           ] ;
               rdfs:range xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Organization
                                     :Person
                                     :Policy
                                     :SecurityRole
                                     :UserIdentity
                                   )
                     ] ;
         rdfs:range xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasPublicationURI
:hasPublicationURI rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Policy
                                               :PrivacyConsentDirective
                                             )
                               ] ;
                   rdfs:range xsd:anyURI ;
                   rdfs:comment "Intended to capture the web location where the document in question is published for human consumption, as opposed to the URI of this ontology element."^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasTimeOfTreatment
:hasTimeOfTreatment rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :InformationReference ;
                    rdfs:range xsd:dateTime .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#hasWrittenSignatureRecorded
:hasWrittenSignatureRecorded rdf:type owl:DatatypeProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain :Consenter ;
                             rdfs:range xsd:boolean .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#identityProofingLevelOfAssurance
:identityProofingLevelOfAssurance rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf :levelOfAssurance ;
                                  rdf:type owl:FunctionalProperty .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#levelOfAssurance
:levelOfAssurance rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :BasicSecurityPolicy
                                              :UserIdentity
                                            )
                              ] ;
                  rdfs:range xsd:integer .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#thirdPartyLevelOfAssurance
:thirdPartyLevelOfAssurance rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf :levelOfAssurance ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain :BasicSecurityPolicy .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#usingWorkstationId
:usingWorkstationId rdf:type owl:DatatypeProperty ;
                    rdfs:domain :UserIdentity ;
                    rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Authority
:Authority rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :JurisdictionalOrganization
                                               :ProviderOrganization
                                             )
                               ] ;
           <http://purl.org/dc/elements/1.1/description> "An authority is an organization (either Jurisdictional or Provider) that is responsible for a Privacy Policy."^^xsd:string ;
           <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
           rdfs:comment "According to the CSP-DAM: 'This abstract class is used to designate the authority that issues the policy.'"^^xsd:string ;
           rdfs:label "Authority"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#AuthorizationPolicy
:AuthorizationPolicy rdf:type owl:Class ;
                     rdfs:subClassOf :BasicSecurityPolicy ;
                     <http://purl.org/dc/elements/1.1/description> "An authorization policy defines allowed actions."^^xsd:string ;
                     <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
                     rdfs:label "Authorization Policy"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#BasicSecurityPolicy
:BasicSecurityPolicy rdf:type owl:Class ;
                     rdfs:subClassOf :SecurityPolicy ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsInformationReference ;
                                       owl:allValuesFrom :InformationReference
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsOperation ;
                                       owl:allValuesFrom :Operation
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsPurposeOfUse ;
                                       owl:allValuesFrom :PurposeOfUse
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsSecurityRole ;
                                       owl:allValuesFrom :SecurityRole
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :restrictedToRoute ;
                                       owl:allValuesFrom :Route
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :authenticationLevelOfAssurance ;
                                       owl:allValuesFrom xsd:integer
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsTimeOfDay ;
                                       owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                           owl:onDatatype xsd:integer ;
                                                           owl:withRestrictions ( [ xsd:minInclusive 0
                                                                                  ]
                                                                                  [ xsd:maxInclusive 86400
                                                                                  ]
                                                                                )
                                                         ]
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :identityProofingLevelOfAssurance ;
                                       owl:allValuesFrom xsd:integer
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :thirdPartyLevelOfAssurance ;
                                       owl:allValuesFrom xsd:integer
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :governsTimeOfDay ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:integer ;
                                                         owl:withRestrictions ( [ xsd:minInclusive 0
                                                                                ]
                                                                                [ xsd:maxInclusive 86400
                                                                                ]
                                                                              )
                                                       ]
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :authenticationLevelOfAssurance ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:integer
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :identityProofingLevelOfAssurance ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:integer
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :thirdPartyLevelOfAssurance ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:integer
                                     ] ;
                     owl:disjointWith :CompositeSecurityPolicy ;
                     <http://purl.org/dc/elements/1.1/description> "A basic security policy serves as a common basis for more specific types of security policies, for example authorization, delegation, obligation and refrain policies."^^xsd:string ;
                     <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
                     rdfs:label "Basic Security Policy"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Clearinghouse
:Clearinghouse rdf:type owl:Class ;
               rdfs:subClassOf :Organization ;
               owl:disjointWith :ProviderOrganization ;
               <http://purl.org/dc/elements/1.1/description> "A clearinghouse is an organization that collects, classifies and/or distributes (healthcare) information."^^xsd:string ;
               <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
               rdfs:comment "Description derived from Merriam Webster."^^xsd:string ,
                            "Mentioned in text of CSP-DAM though not specified there as a class."^^xsd:string ;
               rdfs:label "Clearinghouse"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Client
:Client rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Person
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasPrivacyConsentDirective ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass :PrivacyConsentDirective
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        <http://purl.org/dc/elements/1.1/description> "A client is a person who is the subject of a consent directive. [HL7 CSP-DAM]"^^xsd:string ;
        <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
        rdfs:comment "The definition of Client is debatable.  On the other hand, a client with no consent directive arguably has no impact on access control.\""^^xsd:string ;
        rdfs:label "Client"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#ClinicalObservation
:ClinicalObservation rdf:type owl:Class ;
                     <http://purl.org/dc/elements/1.1/description> "As a part of the service they provide to patients, medical doctors, nurses and allied health workers compile notes that document the conditions they encounter, the treatments provided and the outcomes of those treatments. [U.S. National Library of Medicine; http://www.nlm.nih.gov/nichsr/usestats/clinical_observation.html; Retrieved on July 29, 2013]"^^xsd:string ;
                     <http://purl.org/dc/elements/1.1/source> "HL7 HCS"^^xsd:string ;
                     rdfs:comment "ClinicalObservation is a generalization of the ClinicalCondition class from the HL7 CSP-DAM."^^xsd:string ,
                                  "Subclasses of ClinicalObservation are specified in ClinicalObservationOntology.owl."^^xsd:string ;
                     rdfs:label "Clinical Observation"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Compartment
:Compartment rdf:type owl:Class ;
             <http://purl.org/dc/elements/1.1/description> "A compartment is a security label tag that \"segments\" an IT resource by indicating that access and use is restricted to members of a defined community or project."^^xsd:string ;
             <http://purl.org/dc/elements/1.1/source> "HL7 Healthcare Privacy and Security Classification System (HCS)"^^xsd:string ;
             rdfs:label "Compartment"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#CompositeSecurityPolicy
:CompositeSecurityPolicy rdf:type owl:Class ;
                         rdfs:subClassOf :SecurityPolicy ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:allValuesFrom :BasicSecurityPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :BasicSecurityPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :AuthorizationPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :ConstraintPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :DelegationPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :ObligationPolicy
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :containsSecurityPolicy ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :RefrainPolicy
                                         ] ;
                         <http://purl.org/dc/elements/1.1/description> "A composite security policy combines other policies (authorization, constraint, delegation, obligation, and/or refrain policies)."^^xsd:string ;
                         <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
                         rdfs:label "Composite Security Policy"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Confidentiality
:Confidentiality rdf:type owl:Class ;
                 <http://purl.org/dc/elements/1.1/description> "Confidentiality is the property that information is not made available or disclosed to unauthorized individuals. [per ISO 7498-2:1989]"^^xsd:string ;
                 <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM, which cites ISO 7498-2:1989."^^xsd:string ;
                 rdfs:comment "Subclasses of Confidentiality are specified in ConfidentialityOntology.owl."^^xsd:string ;
                 rdfs:label "Confidentiality"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Consenter
:Consenter rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Person
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isConsenterFor ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Client
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ,
                               [ owl:intersectionOf ( :Person
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty [ owl:inverseOf :hasConsenter
                                                                       ] ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :PrivacyConsentDirective
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasWrittenSignatureRecorded ;
                             owl:allValuesFrom xsd:boolean
                           ] ;
           <http://purl.org/dc/elements/1.1/description> "A Consenter is a person who is an individual client of healthcare services or a client delegate (Substitute Decision Maker) that uses a computer system to manage client privacy consent directives."^^xsd:string ;
           <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
           rdfs:label "Consenter"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Constraint
:Constraint rdf:type owl:Class ;
            <http://purl.org/dc/elements/1.1/description> "A constraint is a restriction (condition or obligation) that is enforced upon access permissions."^^xsd:string ;
            <http://purl.org/dc/elements/1.1/source> "ANSI/INCITS 359-2004"^^xsd:string ;
            rdfs:comment "In the future, subclasses of Constraint may be moved to a separate sub-ontology."^^xsd:string ;
            rdfs:label "Constraint"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#ConstraintCatalog
:ConstraintCatalog rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :catalogsConstraint ;
                                     owl:allValuesFrom :Constraint
                                   ] ;
                   <http://purl.org/dc/elements/1.1/description> "A constraint catalog is a collection of constraints on permissions (as specified by an organization)."^^xsd:string ;
                   <http://purl.org/dc/elements/1.1/source> "HL7 RBAC Constraint Catalog"^^xsd:string ;
                   rdfs:label "Constraint Catalog"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#ConstraintPolicy
:ConstraintPolicy rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :BasicSecurityPolicy
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :constrainsSecurityPolicy ;
                                                               owl:allValuesFrom :CompositeSecurityPolicy
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :specifiesConstraint ;
                                                               owl:allValuesFrom :Constraint
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :specifiesConstraint ;
                                                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onClass :Constraint
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :constrainsSecurityPolicy ;
                                                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onClass :CompositeSecurityPolicy
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  <http://purl.org/dc/elements/1.1/description> "A constraint policy places constraints on another policy."^^xsd:string ;
                  <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
                  rdfs:label "Constraint Policy"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#DataOperation
:DataOperation rdf:type owl:Class ;
               rdfs:subClassOf :Operation ;
               owl:disjointWith :PrivacyOperation ;
               <http://purl.org/dc/elements/1.1/description> "The DataOperation class categorizes operations from the perspective of their effect on object(s)."^^xsd:string ;
               <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
               rdfs:comment "Subclasses of DataOperation are specified in CRUDEA_OperationOntology.owl and in turn in DataOperationOntology.owl."^^xsd:string ;
               rdfs:label "Data Operation"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#DelegationPolicy
:DelegationPolicy rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :BasicSecurityPolicy
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :specifiesGrantee ;
                                                               owl:allValuesFrom :Grantee
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :specifiesGrantee ;
                                                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                               owl:onClass :Grantee
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  <http://purl.org/dc/elements/1.1/description> "A delegation policy defines what authorizations can be delegated to whom."^^xsd:string ;
                  <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
                  rdfs:label "Delegation Policy"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#FunctionalRole
:FunctionalRole rdf:type owl:Class ;
                rdfs:subClassOf :SecurityRole ;
                owl:disjointWith :StructuralRole ;
                <http://purl.org/dc/elements/1.1/description> "A functional role reflects functional aspects of relationships between entities. Functional roles are bound to the realization/performance of acts."^^xsd:string ;
                <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
                rdfs:comment "[taken from HL7 RBAC Role Engineering Process:] Functional Roles consist of all the permissions (operations on health information system objects) needed to perform a task.  Functional role names are associated with groups of permissions for convenience in assigning to users."^^xsd:string ;
                rdfs:label "Functional Role"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Grantee
:Grantee rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                      owl:unionOf ( :Clearinghouse
                                                                    :Person
                                                                    :ProviderOrganization
                                                                  )
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty [ owl:inverseOf :specifiesGrantee
                                                                     ] ;
                                                      owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         <http://purl.org/dc/elements/1.1/description> "A grantee is a person or organization given privilege(s), e.g., by a delegation policy."^^xsd:string ;
         <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
         rdfs:label "Grantee"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#HealthRecord
:HealthRecord rdf:type owl:Class ;
              rdfs:subClassOf :Object ;
              <http://purl.org/dc/elements/1.1/description> "A health record stores healthcare information about a person."^^xsd:string ;
              <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
              rdfs:label "Health Record"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#InformationReference
:InformationReference rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasClinicalObservation ;
                                        owl:allValuesFrom :ClinicalObservation
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasCompartment ;
                                        owl:allValuesFrom :Compartment
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasConfidentiality ;
                                        owl:allValuesFrom :Confidentiality
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasCustodian ;
                                        owl:allValuesFrom :ProviderOrganization
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasIntegrity ;
                                        owl:allValuesFrom :Integrity
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasObject ;
                                        owl:allValuesFrom :Object
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasPaymentSource ;
                                        owl:allValuesFrom :PaymentSource
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasRecipient ;
                                        owl:allValuesFrom :Organization
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasSensitivity ;
                                        owl:allValuesFrom :Sensitivity
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasSubject ;
                                        owl:allValuesFrom :SubjectOfRecord
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasTimeOfTreatment ;
                                        owl:allValuesFrom xsd:dateTime
                                      ] ;
                      <http://purl.org/dc/elements/1.1/description> "An information reference is a specification of the attributes of protected information referenced by a policy."^^xsd:string ;
                      <http://purl.org/dc/elements/1.1/source> "HL7 Composite Security and Privacy DAM"^^xsd:string ;
                      rdfs:label "Information Reference"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#Integrity
:Integrity rdf:type owl:Class ;
           <http://purl.org/dc/elements/1.1/description> "Integrity is the attribute that information is not altered in any way, deliberately or accidentally."^^xsd:string ;
           <http://purl.org/dc/elements/1.1/source> "ISO 22600-2"^^xsd:string ;
           rdfs:comment "Made a meaningless change in the wording of the ISO definition, from 'property' to 'attribute' to avoid confusion with OWL properties."^^xsd:string ,
                        "Subclasses of Integrity are specified in IntegrityOntology.owl."^^xsd:string ;
           rdfs:label "Integrity"^^xsd:string .


###  http://hl7.org/ontology/SecurityAndPrivacyOntology.owl#InterorganizationalAgreement
:InterorganizationalAgreement rdf:type owl:Class ;
                              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasOrganization ;
                                                owl:allValuesFrom :Organization
                                              ] ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasOrganization ;
                                                owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                owl:onClass :Organization
                                              ] ;
