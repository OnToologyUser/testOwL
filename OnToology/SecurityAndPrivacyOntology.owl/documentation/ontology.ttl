                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasPrivacyRule ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :PrivacyRule
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasSubjectOfConsent ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Client
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasEffectiveTime ;
                                           owl:allValuesFrom xsd:dateTime
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasIdentifier ;
                                           owl:allValuesFrom xsd:string
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasPublicationURI ;
                                           owl:allValuesFrom xsd:anyURI
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasIdentifier ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:string
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasEffectiveTime ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:dateTime
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasPublicationURI ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:anyURI
                                         ] ;
                         <http://purl.org/dc/elements/1.1/description> "A privacy consent directive is special policy which defines an agreement between an entity playing the role of the subject of an act and an entity acting. [ISO 22600-3]"^^xsd:string ;
