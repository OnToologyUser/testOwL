                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEffectiveTime ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:dateTime
                             ] ;
             <http://purl.org/dc/elements/1.1/description> "A privacy rule specifies permission or obligation to treat information in accord with privacy wishes."^^xsd:string ;
