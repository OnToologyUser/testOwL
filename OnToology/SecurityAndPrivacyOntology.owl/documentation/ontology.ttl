                            owl:allValuesFrom :Organization
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isLocationForOrganization ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Organization
                          ] ;
          <http://purl.org/dc/elements/1.1/description> "A location is a site (such as a physical place or a system, workstation or terminal in a network)."^^xsd:string ;
